pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
          stage('Build') {
            steps {
                echo 'Hello World build'
            }
        }
          stage('Deploy') {
            steps {
                echo 'Hello World deploy'
            }
        }
          stage('Test') {
            steps {
                echo 'Hello World test'
            }
        }
    }
}
